{
  "name": "eufm",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "ubos",
    "dashboard-react",
    "consultant-portal",
    "packages/*"
  ],
  "scripts": {
    "build": "tsc -p .",
    "start": "node dist/cli/index.js",
    "dev": "tsx src/cli/index.ts",
    "dev:demo": "tsx src/cli/demo_eufm_agents.ts",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test:file-writer": "tsx src/tests/fileWriter_test.ts",
    "test:integration": "tsx src/tests/eufm_integration_test.ts 'Integration check'",
    "test:coord": "tsx src/tests/multiAgentCoordinator_test.ts",
    "test:country-codes": "tsx src/tests/countryCodes_test.ts",
    "dev:oradea": "tsx src/tools/oradea_business_cli.ts",
    "dev:dashboard": "tsx src/dashboard/dashboardServer.ts",
    "analyze:codebase": "tsx src/analysis/analyzeCodebase.ts",
    "fix:security": "tsx src/analysis/fixSecurity.ts",
    "desktop:dev": "electron desktop/electron-main.mjs",
    "dev:dashboard:react": "npm run -C dashboard-react dev",
    "build:dashboard:react": "npm run -C dashboard-react build"
  },
  "dependencies": {
    "@notionhq/client": "^5.0.0",
    "@types/express": "^5.0.3",
    "@types/ws": "^8.18.1",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2",
    "stripe": "^18.5.0",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.19.13",
    "electron": "^30.5.1",
    "eslint": "^8.57.0",
    "jose": "^5.10.0",
    "tsx": "^4.16.2",
    "typescript": "^5.9.2"
  }
}
